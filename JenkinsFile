pipeline {
 agent any
 environment {
  dotnet = 'pathtodotnet.exe'
 }
 stages {
  stage('Checkout') {
   steps {
    git credentialsId: '2d97ff19-7e91-4fc8-89e6-9fc2e855e890', url: 'https://github.com/aayushmaan580/dotnetProject.git', branch: 'master'
   }
  }
  stage('Restore PACKAGES') {
   steps {
    bat "dotnet restore "
   }
  }
  stage('Clean') {
   steps {
    bat 'dotnet clean'
   }
  }
  stage('Build') {
   steps {
    bat 'dotnet build '
   }
  }
  stage('Pack') {
   steps {
    bat 'dotnet pack '
   }
  }
  stage('Publish') {
   steps {
    bat "dotnet publish"
   }
   
  }
 }
}
